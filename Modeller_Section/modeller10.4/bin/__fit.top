# Purpose: superposes MODEL2 on MODEL
# Input  : MODEL, MODEL2, CODE, CODE2, ALNFILE, MODEL2_FIT, etc.
# Output : writes out a file with MODEL2 superposed on MODEL

SUBROUTINE ROUTINE = 'fit'

  READ_MODEL FILE = MODEL
  READ_MODEL2 FILE = MODEL2

  SET ALIGN_CODES = CODE CODE2
  READ_ALIGNMENT FILE = ALNFILE

  PICK_ATOMS PICK_ATOMS_SET = 1, ATOM_TYPES = 'CA'

  SUPERPOSE

  WRITE_MODEL2 FILE = MODEL2_FIT

  RETURN
END_SUBROUTINE
